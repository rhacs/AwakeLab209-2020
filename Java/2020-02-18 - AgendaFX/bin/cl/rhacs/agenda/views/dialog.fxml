<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox maxHeight="180.0" maxWidth="480.0" minHeight="180.0" minWidth="400.0" prefHeight="180.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <HBox>
         <children>
            <Label maxWidth="120.0" minWidth="120.0" prefWidth="120.0" text="Nombre:">
               <HBox.margin>
                  <Insets top="4.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="campoNombre" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Label maxWidth="120.0" minWidth="120.0" prefWidth="120.0" text="TelÃ©fono:">
               <HBox.margin>
                  <Insets top="4.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="campoTelefono" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label maxWidth="120.0" minWidth="120.0" prefWidth="120.0" text="Tipo de Contacto:">
               <HBox.margin>
                  <Insets top="4.0" />
               </HBox.margin>
            </Label>
            <ComboBox fx:id="campoTipo" minWidth="260.0" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
      <AnchorPane VBox.vgrow="ALWAYS" />
      <HBox VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
         <children>
            <AnchorPane HBox.hgrow="ALWAYS" />
            <Button fx:id="botonAceptar" maxWidth="100.0" minWidth="100.0" mnemonicParsing="false" onAction="#handleAceptar" prefWidth="100.0" text="Agregar">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="botonCancelar" maxWidth="100.0" minWidth="100.0" mnemonicParsing="false" onAction="#handleCancelar" prefWidth="100.0" text="Cancelar" />
         </children>
      </HBox>
   </children>
</VBox>
